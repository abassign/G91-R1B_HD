                        } else if (getprop("fdm/jsbsim/systems/autopilot/gui/pid-no") == 1) {
                            kp = 0.2 * ku;
                            if (tu > 0.01) {
                                ki = (2.0/5.0) * ku / tu;
                                kd = (ku / tu) / 15.0;
